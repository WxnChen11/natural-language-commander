{
  "name": "natural-language-commander",
  "version": "0.0.8",
  "description": "A tool for connecting natural language commands with callbacks.",
  "homepage": "https://github.com/whenther/natural-language-commander",
  "bugs": "https://github.com/whenther/natural-language-commander/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/whenther/natural-language-commander.git"
  },
  "main": "dist/nlc.js",
  "scripts": {
    "test": "mocha --opts ./mocha.opts",
    "build": "typings install && tsc && browserify ./dist/NaturalLanguageCommander.js -t [ babelify --presets [ es2015 ] --plugins [ transform-runtime ] ] -o dist/nlc.js --standalone NLC && uglifyjs dist/nlc.js -o dist/nlc.min.js",
    "prebuild": "rimraf dist && mkdirp dist",
    "postbuild": "rimraf dist/test",
    "prepublish": "npm run build"
  },
  "keywords": [
    "natural language processing",
    "nlp",
    "commands",
    "bots",
    "hubot"
  ],
  "author": "Will Lee-Wagner <will@whentheresawill.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.1",
    "rimraf": "^2.5.2",
    "ts-node": "^0.7.3",
    "tsify": "^0.16.0",
    "typescript": "^1.9.0-dev.20160531-1.0",
    "typings": "^1.0.4",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "moment-timezone": "^0.5.3"
  },
  "typings": "./dist/NaturalLanguageCommander.d.ts",
  "engines": {
    "node": "5.10.x"
  }
}
